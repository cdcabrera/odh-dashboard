{
  "name": "frontend",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "build": "react-scripts build",
    "eject": "react-scripts eject",
    "start": "run-p -l start:js",
    "start:js": "react-scripts start",
    "test": "run-s test:lint test:ci",
    "test:ci": "export CI=true; react-scripts test --env=jsdom --roots=./src --coverage",
    "test:clearCache": "react-scripts test --clearCache",
    "test:dev": "run-s test:lint test:local",
    "test:lint": "eslint --ext .json,.js,.jsx,.tsx ./src",
    "test:lint-fix": "eslint --ext .json,.js,.jsx,.tsx ./src --fix",
    "test:local": "react-scripts test --env=jsdom --roots=./src"
  },
  "dependencies": {
    "@patternfly/patternfly": "^4.42.2",
    "@patternfly/react-core": "^4.50.2",
    "@patternfly/react-table": "^4.16.20",
    "@patternfly/react-icons": "^4.7.22",
    "@testing-library/jest-dom": "^5.11.4",
    "@testing-library/react": "^11.0.4",
    "@testing-library/user-event": "^12.1.7",
    "@types/jest": "^26.0.19",
    "@types/node": "^14.14.14",
    "@types/react": "^17.0.0",
    "@types/react-dom": "^17.0.0",
    "@types/react-redux": "7.1.12",
    "@types/react-router-dom": "5.1.6",
    "classnames": "^2.2.6",
    "ky": "^0.24.0",
    "lodash-es": "^4.17.15",
    "node-sass": "^4.14.1",
    "prop-types": "^15.7.2",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-redux": "^7.2.1",
    "react-router": "^5.2.0",
    "react-router-dom": "^5.2.0",
    "react-scripts": "^3.4.3",
    "redux": "^4.0.5",
    "redux-logger": "^3.0.6",
    "redux-promise-middleware": "^6.1.2",
    "redux-thunk": "^2.3.0"
  },
  "devDependencies": {
    "eslint-plugin-jest": "^24.1.3",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-react": "^7.21.5",
    "eslint-plugin-react-hooks": "^4.2.0",
    "redux-mock-store": "^1.5.4",
    "typescript": "^4.1.3"
  },
  "resolutions": {
    "**/eslint": "6.8.0"
  },
  "jest": {
    "coverageThreshold": {
      "global": {
        "branches": 0,
        "functions": 0,
        "lines": 0,
        "statements": 0
      }
    },
    "collectCoverageFrom": [
      "src/**/*.{js,jsx,ts,tsx}"
    ],
    "snapshotSerializers": []
  },
  "proxy": "http://localhost:8080",
  "browserslist": []
}
